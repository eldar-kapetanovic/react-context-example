{
  "name": "react-context-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "firebase": "^7.12.0",
    "history": "^4.10.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-spring": "^8.0.27",
    "toasted-notes": "^3.2.0"
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.14.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "import",
      "react",
      "react-hooks"
    ],
    "rules": {
      "array-callback-return": [
        0
      ],
      "capitalized-comments": [
        0
      ],
      "class-methods-use-this": [
        0
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ],
      "func-names": [
        "error",
        "never"
      ],
      "max-depth": [
        1
      ],
      "max-len": [
        "error",
        250
      ],
      "newline-per-chained-call": [
        0
      ],
      "no-continue": "off",
      "no-else-return": [
        2
      ],
      "no-lonely-if": [
        "error"
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-plusplus": "off",
      "no-restricted-syntax": "off",
      "no-underscore-dangle": "off",
      "no-use-before-define": [
        "warn",
        {
          "functions": false
        }
      ],
      "one-var": [
        0
      ],
      "prefer-arrow-callback": [
        0
      ],
      "prefer-rest-params": [
        0
      ],
      "space-before-blocks": [
        "error",
        "always"
      ],
      "space-before-function-paren": [
        "warn",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "ignore"
        }
      ],
      "spaced-comment": [
        "error",
        "always"
      ],
      "complexity": [
        1,
        8
      ],
      "curly": [
        "error"
      ],
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "quotes": [
        "error",
        "double",
        {
          "avoidEscape": true
        }
      ],
      "radix": [
        "warn",
        "always"
      ],
      "padded-blocks": "off",
      "operator-linebreak": "off",
      "object-curly-newline": [
        "error",
        {
          "ObjectExpression": {
            "consistent": true
          },
          "ObjectPattern": {
            "consistent": true
          },
          "ImportDeclaration": {
            "multiline": true,
            "minProperties": 3
          },
          "ExportDeclaration": {
            "multiline": true,
            "minProperties": 3
          }
        }
      ],
      "implicit-arrow-linebreak": [
        "error",
        "beside"
      ],
      "linebreak-style": 0,
      "function-paren-newline": [
        "error",
        "consistent"
      ],
      "import/no-extraneous-dependencies": [
        0
      ],
      "import/no-unresolved": [
        0
      ],
      "import/extensions": [
        0
      ],
      "react/forbid-prop-types": "off",
      "react/jsx-filename-extension": "off",
      "react/jsx-indent": [
        "error",
        4
      ],
      "react/jsx-indent-props": [
        "error",
        4
      ],
      "react/jsx-one-expression-per-line": [
        "off"
      ],
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
